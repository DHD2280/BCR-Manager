    <string name="app_name" translatable="false">BCR Manager</string>

    <!-- Permission Fragment Strings -->
    <string name="wellcome_title">Welcome to BCR Manager!</string>
    <string name="wellcome_message">This app is for user of BCR Magisk Module. It will simplify your life when searching and playing Call Recordings.\nRemember to set a default BCR directory different from the default one (since this app can\'t read in a folder of other app) and set ON \'Write Json Metadata\' in BCR Settings! JSON Metadata is not mandatory, but will help a lot the app loading recordings. The app tries anyway to parse recordings without JSON.</string>
    <string name="setup_permissions_title">Setup Permissions</string>
    <string name="setup_permissions_call_log_title">Read Call Log</string>
    <string name="setup_permissions_call_log_message">Read Call Log permission is required to check in Call Log the current contact Name.</string>
    <string name="setup_permissions_contact_title">Read Contacts</string>
    <string name="setup_permissions_contact_message">Read Contacts permission is required to check in the Contact Name (if Call Log fails) and Contact Image.</string>
    <string name="setup_permissions_phone_state_title">Read Phone State</string>
    <string name="setup_permissions_phone_state_message">Read Phone State permission is required to check if the number of active SIMs and for checking if the caller is Voice Mail.</string>
    <string name="setup_permissions_grant">Grant Permissions</string>
    <string name="setup_permissions_granted">All done!</string>

